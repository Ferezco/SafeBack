<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tri.felipe.safeback" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher_safeback"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".View.NavigationActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Training.TrainingPagerActivity" >
        </activity>
        <activity
            android:name=".View.Training.SingleImageActivity"
            android:label="@string/title_activity_single_image"
            android:theme="@style/ImageActionBar">
        </activity>
        <activity android:name=".View.Article.ArticleActivity"
            android:label="@string/titile_article_activity"></activity>
    </application>

</manifest>
